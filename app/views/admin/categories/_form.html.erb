<%= form_for(category, url: admin_category_path(category), html: { method: :put } ) do |f| %>

  <%= render partial: "/shared/errors", locals: {record: category} %>

  <ol>
    <li>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </li>
    
  </ol>

  <div class="buttons small cf">
    <%= f.submit "Save", class: "button" %>
  </div>
<% end %>